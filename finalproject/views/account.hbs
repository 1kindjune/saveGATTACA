<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<form class = "dnaform-container" onSubmit = "return confirm('Are you sure you want to delete?');" method = "POST" action = "/account/">

<div class="dnaform-title"><h2> Hello {{firstName}}!!!<h2> </div>
	<ul>
		{{#each dnaStrands}}
			<input type = "checkbox" name = "stuffChecked" value = "{{dnaName}}">
			<a href = /../dna/{{slug}}>{{dnaName}}</a>

			<br />
			<br />

		{{/each}}
	</ul>

	<div class = "center">
	<a class = "btn" href = "/../add">+</a>

	<input type = "submit" class = "btnRed" value = "Delete">

	<button type = "submit" class = "btn" name ="logOut" value = "logout">Log Out</button>
	</div>


</form>


<script>
{{#if nothingDeleted}}
	alert("Nothing Deleted!");
{{/if}}
</script>







